<%= form_with model: @user do |form| %>
<div>
  <%= form.label :FirstName %></br>
  <%= form.text_field :fname %>
</div></br>
<div>
  <%= form.label :LastName %><br/>
  <%= form.text_field :lname %>
</div></br>
<div>
  <%= form.label :Email %><br/>
  <%= form.text_field :email %>
</div></br>
<div>
  <%= form.submit :Create ,class: 'btn btn-success' %>
</div>
<% end %>